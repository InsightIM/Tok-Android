<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/s_1" android:color="@color/chat_et_focus_bg" />
            <corners android:radius="@dimen/s_16" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/s_1" android:color="@color/chat_et_normal_bg" />
            <corners android:radius="@dimen/s_16" />
        </shape>
    </item>
</selector>
