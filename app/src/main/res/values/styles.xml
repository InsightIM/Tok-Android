<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- the theme applied to the application or activity -->
    <style name="AntoxTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="actionBarStyle">@style/ActionBar</item>
        <item name="actionBarTabStyle">@style/ActionBarTabs</item>

        <item name="colorPrimary">@color/brand_primary</item>
        <item name="colorPrimaryDark">@color/brand_primary_dark</item>
        <item name="colorAccent">@color/brand_secondary</item>
    </style>


    <!-- ActionBar styles -->
    <style name="ActionBar" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="android:titleTextStyle">@style/ActionBarTitleText</item>

        <!-- Support library compatibility -->
        <item name="titleTextStyle">@style/ActionBarTitleText</item>

        <!-- Support library compatibility -->
        <item name="background">@color/action_bar_background</item>
        <item name="backgroundStacked">@color/action_bar_background</item>
        <item name="backgroundSplit">@color/action_bar_background</item>
    </style>

    <!-- ActionBar title text -->
    <style name="ActionBarTitleText" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/white</item>
        <!-- The textColor property is backward compatible with the Support Library -->
    </style>

    <style name="ActionBarTabs">
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/tab_background</item>
        <item name="background">@drawable/tab_background</item>
    </style>


    <!-- Base application theme. NEW NEW -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white_absolute</item>
        <item name="colorPrimaryDark">@color/wha</item>
        <item name="colorAccent">@color/brand_secondary</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:windowAnimationStyle">@style/animationLeftToRight</item>
        <item name="android:windowBackground">@color/app_main_bg</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme.Splash" parent="AppTheme">
        <item name="android:windowBackground">@drawable/splash_bg</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="AppTheme.Light" parent="AppTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/wha</item>
        <item name="android:windowBackground">@color/app_main_bg</item>
        <item name="android:textStyle">normal</item>
    </style>

    <!--toolbar title background black-->
    <style name="ToolbarTitleTheme.Black" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">@dimen/font_20</item>
        <item name="android:textColor">@color/wha</item>
        <item name="android:textStyle">normal</item>
    </style>

    <!--toolbar title bg black+big font-->
    <style name="ToolbarTitleTheme.Black.Big" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">@dimen/font_30</item>
        <item name="android:textColor">@color/wha</item>
        <item name="android:textStyle">normal</item>
    </style>

    <!--toolbar title white-->
    <style name="ToolbarTitleTheme.White" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">@dimen/font_20</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">normal</item>
    </style>

    <!--toolbar theme black-->
    <style name="CollapsingToolbarTheme.Black" parent="TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">@dimen/font_8</item>
        <item name="android:textColor">@color/wha</item>
        <item name="android:textStyle">normal</item>
    </style>

    <!--2.0.0 font on Toolbar style black-->
    <style name="ToolbarMenuTheme.Black" parent="@style/ThemeOverlay.AppCompat.ActionBar">
        <item name="actionMenuTextColor">@color/c_ffffff</item>
        <item name="android:textSize">@dimen/font_16</item>
        <item name="android:textStyle">normal</item>
    </style>

    <!--2.0.0 Toolbar-->
    <style name="ToolbarMenuTheme.White" parent="@style/ThemeOverlay.AppCompat.ActionBar">
        <item name="actionMenuTextColor">@color/white</item>
        <item name="android:textSize">@dimen/font_16</item>
        <item name="android:textStyle">normal</item>
    </style>


    <style name="animationLeftToRight" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/slide_in_right</item>
        <item name="android:activityCloseExitAnimation">@anim/slide_out_right</item>
        <item name="android:activityOpenExitAnimation">@anim/slide_out_left</item>
        <item name="android:activityCloseEnterAnimation">@anim/slide_in_left</item>
    </style>

    <!--dialog style-->
    <style name="BaseDialog" parent="android:Theme.Dialog">
        <item name="android:windowIsFloating">true</item>
